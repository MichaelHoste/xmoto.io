	<script>
function OnLoad()	
Game.Message("Welcome to Tix' Wrecker training.")
Game.Message("This level is divided in 3 different parts!")
Game.Message("Thanks to Cigam!")
return true
end
Berry01 = {}
function Berry01.Touch()
x, y = Game.GetEntityPos("TeleportPoint01")
Game.SetPlayerPosition(x, y, 0)
Game.Message("Where is the Sun???")
Game.Message("It's cold!")
end
Berry02 = {}
function Berry02.Touch()
x, y = Game.GetEntityPos("TeleportPoint02")
Game.SetPlayerPosition(x, y, 0)
Game.Message("Look out there is some Lava in this cave!")
end
Berry03 = {}
function Berry03.Touch()
x, y = Game.GetEntityPos("TeleportPoint03")
Game.SetPlayerPosition(x, y, 0)
Game.Message("Now you are back. Nice sun!")
end
Zone01 = {}
function Zone01.OnEnter()
Game.KillPlayer()
end
Zone02 = {}
function Zone02.OnEnter()
Game.Message("Now collect the last Berry to finish the Training!")
end
	</script>
