	<script>
ReloadTime = 3.0
LastChange = -ReloadTime

function GravityChange()
	if Game.GetTime() - LastChange &lt; ReloadTime then
		Game.Message("Not enough energy !")
	else
		x,y = Game.GetGravity()
		Game.SetGravity(x, -y)
		LastChange = Game.GetTime()
	end
	Game.ClearMessages()
end

function OnLoad()
	Game.SetKeyHook("G", "GravityChange")
	Game.Message("Press G for the antigravitation system !");
	Game.Message("Be cautious; it needs 3 seconds to reload...");
	return true
end

	</script>
