	<info>
		<name>Freestyle under fire</name>
		<description>20 somersaults to win !</description>
		<author>Nicolas Adenis-Lamarre</author>
		<date>2006-09-26</date>
		<sky>sky1</sky>
	</info>
	<script>
		todo = 20
function OnLoad()
  n = 0
  Game.Message(todo.." somersaults to finish the level")
  Game.SetDynamicEntityRotation("fire0", 0, 2, 500, 0, 0)
  Game.SetDynamicEntityRotation("fire1", 0, 2, 500, 0, 0)
  return true
end
function OnSomersault(counterclock)
  n = n+1
  if(n == todo)
  then
    Game.WinPlayer()
  end
  if(math.mod(n, 5) == 0)
  then
    Game.Message((todo-n).." somersaults left !")
  end  
end
</script>
